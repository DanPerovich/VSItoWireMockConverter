<?xml version="1.0" encoding="UTF-8"?>
<serviceImage name="multiple_transactions" version="2.0" buildNumber="2.0.0">
  <info>
    <description>Multiple REST transactions for testing</description>
    <tags>REST,JSON,MultipleTransactions,HTTPS</tags>
  </info>
  <transactions>
    <t id="GET#/users">
      <rq>
        <m>
          <method>GET</method>
          <path>/users</path>
          <protocol>http</protocol>
          <headers>
            <header name="Content-Type">application/json</header>
          </headers>
        </m>
      </rq>
      <rs>
        <rp id="success">
          <m>
            <status>200</status>
            <latency ms="100-200">range</latency>
            <weight>0.8</weight>
          </m>
          <bd><![CDATA[{"users": [{"id": 1, "name": "Alice"}, {"id": 2, "name": "Bob"}]}]]></bd>
        </rp>
        <rp id="not_found">
          <m>
            <status>404</status>
            <latency ms="50">fixed</latency>
            <weight>0.2</weight>
          </m>
          <bd><![CDATA[{"error": "No users found"}]]></bd>
        </rp>
      </rs>
    </t>
    
    <t id="POST#/users">
      <rq>
        <m>
          <method>POST</method>
          <path>/users</path>
          <protocol>http</protocol>
          <headers>
            <header name="Content-Type">application/json</header>
          </headers>
        </m>
        <bd><![CDATA[{"name": "Charlie", "email": "charlie@example.com"}]]></bd>
      </rq>
      <rs>
        <rp id="created">
          <m>
            <status>201</status>
            <latency ms="150-300">range</latency>
            <weight>0.9</weight>
          </m>
          <bd><![CDATA[{"id": 3, "name": "Charlie", "email": "charlie@example.com"}]]></bd>
        </rp>
        <rp id="bad_request">
          <m>
            <status>400</status>
            <latency ms="100">fixed</latency>
            <weight>0.1</weight>
          </m>
          <bd><![CDATA[{"error": "Invalid email format"}]]></bd>
        </rp>
      </rs>
    </t>
    
    <t id="DELETE#/users/{id}">
      <rq>
        <m>
          <method>DELETE</method>
          <path>/users/1</path>
          <protocol>http</protocol>
          <headers>
            <header name="Content-Type">application/json</header>
          </headers>
        </m>
      </rq>
      <rs>
        <rp id="deleted">
          <m>
            <status>204</status>
            <latency ms="80-120">range</latency>
            <weight>0.7</weight>
          </m>
        </rp>
        <rp id="not_found">
          <m>
            <status>404</status>
            <latency ms="50">fixed</latency>
            <weight>0.3</weight>
          </m>
          <bd><![CDATA[{"error": "User not found"}]]></bd>
        </rp>
      </rs>
    </t>
  </transactions>
</serviceImage>
