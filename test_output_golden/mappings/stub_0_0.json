{
  "priority": 0,
  "request": {
    "method": "GET",
    "urlPath": "/users",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "users": [
        {
          "id": 1,
          "name": "John"
        },
        {
          "id": 2,
          "name": "Jane"
        }
      ]
    },
    "delayDistribution": {
      "type": "uniform",
      "lower": 100,
      "upper": 200
    },
    "transformers": [
      "response-template"
    ]
  }
}