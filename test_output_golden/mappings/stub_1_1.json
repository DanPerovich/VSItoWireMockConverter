{
  "priority": 1,
  "request": {
    "method": "GET",
    "urlPath": "/users",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 404,
    "jsonBody": {
      "error": "Users not found"
    },
    "fixedDelayMilliseconds": 50,
    "transformers": [
      "response-template"
    ]
  }
}